{
  "name": "simple-photobooth",
  "version": "1.0.0",
  "description": "Photobooth",
  "main": "server.js",
  "directories": {
    "test": "test"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "scripts": {
    "build": "npm run clean && webpack",
    "clean": "rimraf public/dist",
    "lint": "eslint *.js src",
    "prettier": "prettier '*.js' 'src/**/*.js' --write",
    "prettierlint": "npm run prettier && npm run lint",
    "dist": "npm run clean && npm run lint && npm test && cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "productionbuild": "npm run clean && cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "start": "npm run build && nodemon --ignore src --inspect server.js",
    "production": "npm run productionbuild && cross-env NODE_ENV=production node server.js",
    "serve": "cross-env NODE_ENV=production node server.js",
    "nodeonly": "node --inspect server.js",
    "watcher": "node watcher.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FakeNicho/photobooth.git"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Nicho",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "body-parser": "1.18.2",
    "breakpoint-sass": "2.7.1",
    "connected-react-router": "4.3.0",
    "css-loader": "0.28.11",
    "del": "3.0.0",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "3.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "flow-bin": "0.71.0",
    "flow-typed": "2.4.0",
    "history": "4.7.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "jquery": "3.3.1",
    "lint-staged": "7.0.5",
    "moment": "2.22.1",
    "node-sass": "4.9.0",
    "nodemon": "1.17.3",
    "optimize-css-assets-webpack-plugin": "4.0.0",
    "prettier": "1.12.1",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "4.1.2",
    "react-linkify": "0.2.2",
    "react-modal": "3.4.4",
    "react-redux": "5.0.7",
    "react-responsive": "4.1.0",
    "react-router-dom": "4.2.2",
    "react-time": "4.3.0",
    "react-transform-hmr": "1.0.4",
    "redux": "4.0.0",
    "redux-devtools": "3.4.1",
    "redux-devtools-extension": "2.13.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "urijs": "1.19.1",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.15",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.1"
  },
  "dependencies": {
    "chokidar": "2.0.3",
    "cookie-parser": "1.4.3",
    "cross-env": "5.1.4",
    "express": "4.16.3",
    "form-data": "2.3.2",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "8.2.1",
    "morgan": "1.9.0",
    "multer": "1.3.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0"
  }
}
